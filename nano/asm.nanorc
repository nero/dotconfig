## Here is an example for assembler.

syntax "asm" "\.(S|s|ASM|asm|INC|inc)$"
magic "assembler source"

color brightblue "^\S*"
color brightred "\s*%\S*"
color brightgreen "^\s*(section|absolute)\S*"
color cyan "\s*;.*$" "\s*//.*$"
color brightyellow ""(\\.|[^"])*"" "'(\\.|[^'])*'"

color ,green "[[:space:]]+$"
color ,red "\x09"
